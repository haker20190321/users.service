    "coverage_percents": "nyc check-coverage --lines 80 --functions 80 --branches 80"
  },
  "author": "kmayer",
  "license": "ISC",
  "dependencies": {
    "@esoft_private/esoft-service": "^1.0.0",
    "knex": "^0.16.3",
    "pg": "^7.7.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "eslint": "^5.11.1",
    "eslint-config-defaults": "^9.0.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "openapi3-generator": "^0.9.1"
  },
  "nyc": {
    "exclude": [
      "test",
      "coverage"
    ],
    "reporter": [
      "json",
      "html",
      "text",
      "lcovonly"
    ]
